extern void print_int(int n);
extern void print_string(char s[]);
extern int read_int(void);
extern void read_string(char s[]);

/* print_string wrapper */
void ps(char string[]) {
	print_string(string);
}

/* print_int wrapper */
void pi(int n) {
	print_int(n);
}

/* print newline wrapper */
void pn(void) {
	print_string("\n");
}

/* read_int wrapper */
int ri(void) {
	return read_int();
}

/* read_string wrapper */
void rs(char s[]) {
	read_string(s);
}

int n;
char s[100];

void main(void) {
	ps("Enter a number: ");
	n = rn();
	ps("The number you entered is ");
	pi(n);
	pn();
	ps("Enter a string: ");
	rs(s);
	ps("The string you entered is ");
	ps(s);
}